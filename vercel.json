{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron/subscriptionCron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(auth|member|dashboard|admin|finance|test)(.*)",
      "dest": "/api/app.js"
    },
    {
      "src": "/",
      "dest": "/api/app.js"
    },
    {
      "src": "/api/cron/subscriptionCron",
      "dest": "/api/cron/subscriptionCron.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/subscriptionCron",
      "schedule": "0 12 * * *"
    }
  ]
}
